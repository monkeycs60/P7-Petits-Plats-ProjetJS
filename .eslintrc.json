{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "mjs": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
   ],
    "no-plusplus": "off",
    "import/prefer-default-export": "off",
    "radix": "off",
    "no-console": "off",
    "import/no-cycle": "off",
    "no-lonely-if": "off",
    "import/no-mutable-exports": "off",
    "prefer-const": "off",
    "no-param-reassign": "off"
    }
}
